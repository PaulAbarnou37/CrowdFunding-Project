<section class="dashboard-page">
<div class="whatsup">
<h2>What's up {{userMe}}?</h2>
</div>
<div class="my-everything">

{{> flash_messages}} 
<div class="my-project">
<h2 style= 'padding: 30px 0px'>My Projects</h2>
{{!-- <div class="project-list"> --}}

<ul>
  {{#each userProjects}}
  <li>
  <a href= "/projects-list/{{this._id}}"><img src="{{this.pictureUrl}}" height="185px" width ="245px" /></a>
  <a href= "/projects-list/{{this._id}}">{{this.projectName}}</a>
  </li>
  {{/each}}
</ul>
</div>

<div class="my-contributions">
<h2  style= 'padding: 30px 0px'>My Contributions</h2>
<ul>
  {{#each userContributions}}
  <li>
  <p>You contributed ${{this.amount}} to <a href="/projects-list/{{this._id}}">{{this.project.projectName}}</a> on {{this.project.getCreatedAt}}</p>


  </li>
  {{/each}}
</ul>
</div>
{{!-- </div> --}}

<br/>
</div>


<button><a href="/start-a-project">Start a new project</a></button>

</section>


